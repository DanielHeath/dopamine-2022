  bindsym z $send z z
  bindsym x $send z x
  bindsym c $send z c
  bindsym v $send z v
  bindsym b $send z b
  bindsym n $send z n
  bindsym m $send z m

  bindsym 0 $send z 0
  bindsym 1 $send z 1
  bindsym 2 $send z 2
  bindsym 3 $send z 3
  bindsym 4 $send z 4
  bindsym 5 $send z 5
  bindsym 6 $send z 6
  bindsym 7 $send z 7
  bindsym 8 $send z 8
  bindsym 9 $send z 9

  bindsym space mode default
  bindsym escape mode default
}

